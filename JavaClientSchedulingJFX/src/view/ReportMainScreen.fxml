<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportMainScreenController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="totalappt" layoutX="32.0" layoutY="158.0" mnemonicParsing="false" onAction="#totalAppointmentsScreen" prefHeight="56.0" prefWidth="223.0" text="TOTAL CUSTOMER APPOINTMENTS">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="contanctschedule" layoutX="272.0" layoutY="158.0" mnemonicParsing="false" onAction="#contactScheduleReport" prefHeight="56.0" prefWidth="144.0" text="CONTACT SCHEDULES">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="userschedules" layoutX="430.0" layoutY="158.0" mnemonicParsing="false" onAction="#userScheduleScreenButtonHandler" prefHeight="56.0" prefWidth="182.0" text="USER SCHEDULES" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="backbutton" cancelButton="true" layoutX="291.0" layoutY="235.0" mnemonicParsing="false" onAction="#returntoMainMenu" text="BACK" />
                <Text layoutX="474.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="REPORTS MENU">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Text>
                <Text layoutX="254.0" layoutY="141.0" strokeType="OUTSIDE" strokeWidth="0.0" text="SELECT AN OPTION">
                    <font>
                        <Font name="System Bold" size="13.0" />
                    </font>
                </Text>
            </children>
        </AnchorPane>
    </children>
</VBox>
